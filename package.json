{
  "name": "tap-vote-ng",
  "version": "0.0.0",
  "scripts": {
    "all-tests": "npm run lint && npm run test-headless && npm run e2e-headless",
    "build": "ng build --aot --prod && npm run build-app-server",
    "build-dev": "ng build && npm run build-app-server",
    "build-app-server": "tsc app-server.ts --outDir dist/",
    "build-storybook": "build-storybook",
    "dev": "ng serve",
    "e2e": "ng e2e",
    "e2e-headless": "ng e2e --protractor-config='e2e/ci.protractor.conf.js'",
    "lint": "ng lint",
    "ng": "ng",
    "prettier": "pretty-quick",
    "start": "curl https://tap-vote-server.herokuapp.com/api/v1/ping & node dist/app-server.js",
    "storybook": "start-storybook -p 6006",
    "test": "ng test --codeCoverage=true",
    "test-headless": "ng test --codeCoverage=true --watch=false --browsers=ChromeHeadless"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.1.9",
    "@angular/cdk": "^9.2.4",
    "@angular/common": "^9.1.9",
    "@angular/compiler": "^9.1.9",
    "@angular/core": "^9.1.9",
    "@angular/forms": "^9.1.9",
    "@angular/material": "^9.2.4",
    "@angular/platform-browser": "^9.1.9",
    "@angular/platform-browser-dynamic": "^9.1.9",
    "@angular/router": "^9.1.9",
    "express": "^4.17.1",
    "firebase": "^7.14.5",
    "firebase-admin": "^8.12.1",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "uuid": "^8.1.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.901.7",
    "@angular/cli": "^9.1.7",
    "@angular/compiler-cli": "^9.1.9",
    "@babel/core": "^7.9.6",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@storybook/addon-actions": "^5.3.19",
    "@storybook/addon-links": "^5.3.19",
    "@storybook/addon-notes": "^5.3.19",
    "@storybook/addons": "^5.3.19",
    "@storybook/angular": "^5.3.19",
    "@types/express": "^4.17.6",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.12.42",
    "@types/uuid": "^8.0.0",
    "babel-loader": "^8.1.0",
    "codelyzer": "^5.1.2",
    "husky": "^4.2.5",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^6.3.14",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~3.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "protractor": "~5.4.3",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~3.8.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Tap-Vote/Tap-Vote-Ng.git"
  },
  "author": "Blaise Schaeffer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Tap-Vote/Tap-Vote-Ng/issues"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
